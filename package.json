{
  "title": "Ember Uploader",
  "name": "ember-uploader",
  "version": "1.3.1",
  "description": "Ember.js addon to facilitate uploading",
  "homepage": "https://github.com/benefitcloud/ember-uploader",
  "license": "MIT",
  "author": "Joshua Borton <digitaltoad@gmail.com>",
  "repository": "git://github.com/benefitcloud/ember-uploader.git",
