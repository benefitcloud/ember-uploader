{
  "extends" : "airbnb/base",
  "env"     : {
    "browser" : true
  },
  "globals" : {
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__DEBUG_NW__" : false,
    "__API_URL__"  : false,
    "__BASENAME__" : false
  },
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle" : [1, "never"],
    "func-names"   : 0,
    "key-spacing"  : 0,
    "space-before-function-paren" : [2, "always"],
    "no-else-return"   : 0,
    "no-multi-spaces"  : 0,
    "quotes"           : [2, "single"],
    "jsx-quotes"       : [2, "prefer-single"],
    "one-var"          : 0,
    "no-param-reassign": ["error", { "props": false }]
  }
}
